#!/usr/bin/expect


#puts "\n*** Waiting 600 seconds for VM-series instance to come up. ***\n"
#sleep 500 

set pass [lindex $argv 2]
puts "$pass"

set timeout 20
set stty_init "rows 150 cols 188"
spawn -nottycopy ssh "[lindex $argv 1]@[lindex $argv 0]"

expect {
    "yes/no" {send "yes\r"}
    "*?assword: " {send "$pass\r"}
}
expect {
    "> $" {send "\r"}
    "*?assword: " {send "$pass\r"}
}
expect {
    "> $" {send "\r"}
    "*?assword: " {send "$pass\r"}
}
sleep 2
expect "> $" {send "configure\r"}
sleep 2
expect "# $" {send "commit\r"}
sleep 2
expect "# $" {send "exit\r"}
sleep 2
expect "> $" {send "exit\r"}
